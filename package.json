{
  "name": "rice-transaction-app",
  "version": "1.0.0",
  "description": "Simple rice transaction management application",
  "main": "dist/app.js",
  "scripts": {
    "dev": "NODE_OPTIONS='--dns-result-order=ipv4first' UV_THREADPOOL_SIZE=4 ts-node-dev --respawn --transpile-only src/app.ts",
    "build": "tsc",
    "start": "NODE_OPTIONS='--dns-result-order=ipv4first' node dist/app.js",
    "migrate": "node dist/config/migrate.js",
    "vercel-build": "npm run build"
  },
  "keywords": [
    "rice",
    "transaction",
    "auth"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
